{
    "name": "middleware-backend",
    "version": "1.0.1",
    "description": "Dynamic Express API Host",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "install-lib": "node install-lib.js",
        "postinstall": "npm run install-lib && npm run migrate:up",
        "migrate:create": "migrate-mongo create",
        "migrate:up": "migrate-mongo up",
        "migrate:down": "migrate-mongo down",
        "migrate:status": "migrate-mongo status"
    },
    "dependencies": {
        "express": "^4.18.2",
        "migrate-mongo": "^14.0.7"
    }
}